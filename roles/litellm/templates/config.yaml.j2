model_list:
{% for model in litellm_models %}
  - model_name: {{ model.model_name }}
    litellm_params:
{%   for key, value in model.litellm_params.items() %}
      {{ key }}: {{ value }}
{%   endfor %}
{% endfor %}

litellm_settings:
  cache: True
  cache_params:
    type: local

general_settings:
{% if litellm_master_key is not none %}
  master_key: "os.environ/MASTER_KEY"
{% endif %}
  timeout: {{ litellm_timeout }}
